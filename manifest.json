{
  "manifest_version": 2,
  "name": "LegalOne enhancement suite",
  "description": "This extension enhances the experience of LegalOne",
  "version": "1.0",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": [
        "*://*.novajus.com.br/*"
      ],
      "js": [
        "jquery-3.1.0.min.js",
        "main.entry.js"
      ],
      "css": [
        "./css/l1es.css"
      ],
      "run_at": "document_end"
    }
  ],
  "permissions": [
    "activeTab"
  ],
  "icons": {
    "48": "icon.png",
    "128": "icon.png"
  }
}